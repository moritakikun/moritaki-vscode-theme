	"tokenColors": [
    {
      "name": "Tag Arrow HTML",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "text.html.vue-html meta.tag.block.any.html"
      ],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Tag, Component, Class, Interface, Type",
      "scope": [
        "entity.name.tag",
        "entity.name.type.interface",
        "entity.name.type",

        "meta.tag.sgml",
        "meta.type.declaration",
        "meta.interface ",
        "meta.class",

        "markup.deleted.git_gutter",
        "support.class.component.html",

        "new.expr meta.function-call",
        "new.expr meta.brace.round"
      ],
      "settings": {
        "foreground": "#55b0ff"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#828082"
      }
    },
    {
      "name": "Vue template",
      "scope": ["entity.name.tag.template", "entity.name.tag.script", "entity.name.tag.style"],
      "settings": {
        "foreground": "#b47fff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables, Attributes",
      "scope": [
        "variable",

        "punctuation.definition.entity.css",

        "meta.at-rule.mixin",
        "meta.at-rule.extend",

        "entity.other.attribute-name",
        "entity.name.function.ts",
        "entity.name.tag.reference"
      ],
      "settings": {
        "foreground": "#b47fff"
      }
    },
    {
      "name": "Css Variable",
      "scope": [
        "source.css variable",
        "source.sass variable",
        "source.scss variable",
        "source.scss variable",
        "source.less variable",
        "source.stylus variable",
        "source.postcss variable"
      ],
      "settings": {
        "foreground": "#55b0ff"
      }
    },
    {
      "name": "Keyword, Class, Type, Interface, Namespace",
      "scope": [
        "keyword.package",
        "keyword.import",
        "keyword.function",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.new",

        "constant.language",

        "storage.type",
        "storage.modifier",

        "meta.class storage.type.class",
        "meta.use",

        "support.type",
        "support.class",
        "support.interface",
        "support.other.namespace",

        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#7bd2ce"
      }
    },
    {
      "name": "Function, Special Method, Number, Constant",
      "scope": [
        "meta.definition.function",
        "meta.function.expression",
        "meta.function-call",
        "meta.class meta.definition.method",
        "meta.class storage.type",

        "variable.function",
        "support.function",

        "constant.numeric",
        "constant.character",
        "constant.escape",

        "keyword.other"
      ],
      "settings": {
        "foreground": "#ebc88d"
      }
    },
    {
      "name": "Object, Object Member, Variable Other, Operators",
      "scope": [
        "meta.object.member",
        "meta.block",
        "meta.brace.square",
        "meta.brace.round",
        "meta.parameters",
        "meta.objectliteral",

        "variable.other.property",
        "variable.other.object",
        "variable.other.readwrite.alias",
        "variable.other.object.property",
        "variable.other.constant.object.property",

        "storage.type.function.arrow",
        "constant.other.color",

        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance",
        "punctuation.section.embedded",

        "keyword.other.template",
        "keyword.other.substitution",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.type",
        "keyword.operator.optional"
      ],
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "name": "This",
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "#cc6673",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading, Invalid",
      "scope": [
        "string",
        "string.quoted.double",
        "string.unquoted.label",

        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",

        "constant.other.symbol",
        "constant.other.key",

        "markup.heading",
        "markup.inserted.git_gutter",

        "meta.group.braces.curly constant.other.object.key.js",
        "meta.attribute.href",
        "meta.tag.metadata.link.void",

        "support.constant",
        "invalid"
      ],
      "settings": {
        "foreground": "#8ddb8c"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#7bd2ce"
      }
    },
    {
      "name": "Underline",
      "scope": [
        "*url*",
        "*link*",
        "*uri*",
        "meta.import string.quoted.double",
        "meta.import string.quoted.single"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
