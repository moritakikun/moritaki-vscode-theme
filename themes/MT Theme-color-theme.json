	"tokenColors": [
    {
      "name": "Tag arrow",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "text.html.vue-html meta.tag.block.any.html"
      ],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Tag, Component, Class, interface, type",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "support.class.component.html",
        "meta.class",
        "new.expr meta.function-call",
        "new.expr meta.brace.round",
        "entity.name.type.interface",
        "entity.name.type",
        "meta.type.declaration"
      ],
      "settings": {
        "foreground": "#55b0ff"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#828082"
      }
    },
    {
      "name": "Vue template",
      "scope": [
        "entity.name.tag.template",
        "entity.name.tag.script",
        "entity.name.tag.style"
      ],
      "settings": {
        "foreground": "#b47fff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables, Attributes",
      "scope": [
        "variable",
        "variable.parameter",
        "meta.parameters",
        "entity.other.attribute-name",
        "punctuation.definition.entity.css",
        "meta.at-rule.mixin",
        "entity.name.tag.reference",
        "meta.at-rule.extend"
      ],
      "settings": {
        "foreground": "#b47fff"
      }
    },
    {
      "name": "Variables Scss",
      "scope": [
        "variable.scss"
      ],
      "settings": {
        "foreground": "#55b0ff"
      }
    },
    {
      "name": "Keyword Control, New, Class, Type, Interface, Namespace",
      "scope": [
        "keyword.control",
        "keyword.operator.expression.of",
        "constant.language",
        "storage.type",
        "meta.class storage.type.class",
        "keyword.operator.new",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#7bd2ce"
      }
    },
    {
      "name": "Function, Special Method, Number, Constant",
