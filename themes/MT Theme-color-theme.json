        "meta.definition.function",
        "meta.function.expression",
        "meta.function-call",
        "meta.class meta.definition.method",
        "meta.class storage.type",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "constant.numeric",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ebc88d"
      }
    },
    {
      "name": "Object, Object Member, Variable Other, Operators",
